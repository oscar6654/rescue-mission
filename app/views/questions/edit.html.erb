<h1>Edit Question</h1>
<%=form_for(@question_edit, url: question_path(@question_edit)) do |f| %>
  <label for="title">Title</label>
  <%= f.text_field :title, type: "text"%>
  <label for="description">Description</label>
  <%= f.text_field :description, type: "text"%>


  <%= submit_tag "Edit Question", class: 'btn' %>
<%end%>
